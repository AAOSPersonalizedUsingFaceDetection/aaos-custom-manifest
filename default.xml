<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Reference the main AOSP source -->
  <remote name="aosp"
          fetch="https://android.googlesource.com/" />

  <!-- Your GitHub org -->
  <remote name="github"
          fetch="https://github.com/" />

  <default revision="android-14.0.0_r67"
           remote="aosp"
           sync-j="4" />

  <!-- Include Raspberry Pi Vanilla base -->
  <include name="manifest_brcm_rpi.xml" />
  <include name="remove_projects.xml" />

  <!-- Your custom components -->
  <project name="/AAOSPersonalizedUsingFaceDetection/PersonalizedAndroidApp"
           path="packages/apps"
           remote="github"
           revision="main" />

  <project name="/AAOSPersonalizedUsingFaceDetection/PureCamStream"
           path="frameworks/apps"
           remote="github"
           revision="main" />



</manifest>
